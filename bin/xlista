#!/bin/sh
VALUE=false
if [ -z "$1" ]; then
	nada
else
	if [ -z "$EXCL" ]; then EXCL="$(asetus dir:excl)/default.lst"
	#case
			#*)	 nada;;

	#esac
	N=$(cat $EXCL | grep "$1" | wc -l | tr -s " ")
	if [ $N = 0 ]; then VALUE=false; else VALUE=true; fi
	fi
	printf "$VALUE\n"; #virhe "$VALUE" "$EXCL"
fi
