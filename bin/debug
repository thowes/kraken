#!/bin/bash

if [ "_$(echo $KR_DEBUG)_" == "__" ]; then
	KR_DEBUG=$(asetus bool:debug)
fi
if [ $KR_DEBUG == "true" ]; then
	if [ "_$1_" != "__" ]; then
		if [ "_$2_" != "__" ]; then
			if [ "_$3_" != "__" ]; then
				if [ "_$4_" != "__" ]; then
					echo "[[ DEBUG $1 $2 $3 $4 ]]"
				else
					echo "[[ DEBUG $1 $2 $3 ]]"
				fi
			else
				echo "[[ DEBUG $1 $2 ]]"
			fi
		else
			echo "[[ DEBUG $1 ]]"
		fi
	else
		echo "[[ DEBUG ]]"
	fi
fi
