#!/bin/bash

if [ "_$(echo $KR_DEBUG)_" == "__" ]; then
	KR_DEBUG=$(asetus bool:debug)
fi
if [ $KR_DEBUG == "true" ]; then
	if [ "_$1_" != "__" ]; then
		if [ "_$2_" != "__" ]; then
			if [ "_$3_" != "__" ]; then
				if [ "_$4_" != "__" ]; then
					printf "[[ DEBUG $1 $2 $3 $4 ]]\n"
				else
					printf "[[ DEBUG $1 $2 $3 ]]\n"
				fi
			else
				printf "[[ DEBUG $1 $2 ]]\n"
			fi
		else
			printf "[[ DEBUG $1 ]]\n"
		fi
	else
		printf "[[ DEBUG ]]\n"
	fi
fi
