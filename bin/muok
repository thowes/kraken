#!/bin/bash
if [ $KR_DEBUG == "true" ]; then tynnyri MUOK; fi
EDITOR='nano -w -T 2'

muokkaa() {
	EDITOR='nano -w -T 2'
	case $1 in
		em) EDITOR=emacs; . $KR_DIR_CFG/nano.sh $2 $3;;
		emacs) EDITOR=emacs; . $KR_DIR_CFG/nano.sh $2 $3;;
		n) EDITOR=nano; . $KR_DIR_CFG/nano.sh $2 $3;;
		*) . $KR_DIR_CFG/nano.sh $1 $2;;
	esac
	EDITOR='nano -w -T 2'
}